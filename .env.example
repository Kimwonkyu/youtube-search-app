# YouTube API 키 (Google Cloud Console에서 발급)
YOUTUBE_API_KEY=your_youtube_api_key_here

# Google Sheets ID (스프레드시트 URL에서 추출)
SPREADSHEET_ID=your_spreadsheet_id_here

# Google 서비스 계정 인증 정보 (배포용)
# 로컬 개발시에는 credentials.json 파일 사용
# 배포시에는 JSON 전체를 한 줄로 입력
GOOGLE_CREDENTIALS={"type":"service_account","project_id":"your_project_id"...}